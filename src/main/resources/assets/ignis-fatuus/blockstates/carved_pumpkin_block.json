{
    "multipart": [
        {
            "when": {
                "lid": "true"
            },
            "apply": {
                "model": "ignis-fatuus:block/pumpkin_lid"
            }
        },
        {
            "when": {
                "facing": "north"
            },
            "apply": {
                "model": "ignis-fatuus:block/carved_pumpkin_base"
            }
        },
        {
            "when": {
                "facing": "east"
            },
            "apply": {
                "model": "ignis-fatuus:block/carved_pumpkin_base",
                "y": 90
            }
        },
        {
            "when": {
                "facing": "south"
            },
            "apply": {
                "model": "ignis-fatuus:block/carved_pumpkin_base",
                "y": 180
            }
        },
        {
            "when": {
                "facing": "west"
            },
            "apply": {
                "model": "ignis-fatuus:block/carved_pumpkin_base",
                "y": 270
            }
        },
        {
            "when": {
                "facing": "north"
            },
            "apply": {
                "model": "ignis-fatuus:block/pumpkin_dynamic",
                "y": 90
            }
        },
        {
            "when": {
                "facing": "east"
            },
            "apply": {
                "model": "ignis-fatuus:block/pumpkin_dynamic",
                "y": 90
            }
        },
        {
            "when": {
                "facing": "south"
            },
            "apply": {
                "model": "ignis-fatuus:block/pumpkin_dynamic",
                "y": 180
            }
        },
        {
            "when": {
                "facing": "west"
            },
            "apply": {
                "model": "ignis-fatuus:block/pumpkin_dynamic",
                "y": 270
            }
        }
    ]
}